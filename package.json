{
	"name": "express-webpack",
	"version": "1.0.0",
	"description": "Example project that combines TypeScript WebPack and Express.",
	"author": "Simon Mika",
	"license": "MIT",
	"homepage": "https://github.com/cogneco/express-webpack#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cogneco/express-webpack.git"
	},
	"bugs": {
		"url": "https://github.com/cogneco/express-webpack/issues"
	},
	"main": "build/server/Server.js",
	"scripts": {
		"prebuild": "npm run clean",
		"build": "npm run build-client && npm run build-server",
		"build-client": "webpack --config source/webpack.config.js",
		"build-server": "tsc -p source/server/",
		"prestart-client": "npm run build-client",
		"start-client": "NODE_ENV=development webpack-dev-server",
		"prestart-server": "npm run build-server",
		"start-server": "node build/server/Server.js",
		"prestart": "npm run clean",
		"start": "npm run start-server | npm run start-client",
		"clean": "rm -rf build"
	},
	"devDependencies": {
		"@types/express": "^4.0.34",
		"@types/extract-text-webpack-plugin": "^2.0.0",
		"@types/html-webpack-plugin": "^2.22.4",
		"@types/node": "^6.0.51",
		"@types/webpack": "^2.0.0",
		"@types/webpack-dev-server": "^1.12.5",
		"@types/webpack-env": "^1.12.1",
		"css-loader": "^0.26.0",
		"extract-text-webpack-plugin": "^2.0.0-beta.4",
		"file-loader": "^0.9.0",
		"html-webpack-plugin": "^2.24.1",
		"json-loader": "^0.5.4",
		"path": "^0.12.7",
		"style-loader": "^0.13.1",
		"ts-loader": "^1.2.0",
		"ts-node": "^1.7.0",
		"tslint": "^4.0.2",
		"tslint-react": "^2.0.0",
		"typescript": "^2.0.10",
		"url-loader": "^0.5.7",
		"webpack": "^2.1.0-beta.27",
		"webpack-dev-middleware": "^1.8.4",
		"webpack-dev-server": "^2.1.0-beta.12",
		"webpack-hot-middleware": "^2.13.2"
	},
	"dependencies": {
		"express": "^4.14.0",
		"node": "0.0.0"
	}
}
