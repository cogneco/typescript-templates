{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"sourceRoot": "../",
		"outDir": "../../build/",
		"types": [
			"node"
		]
	},
	"exclude": [
		"node_modules"
	],
	"files": [
		"Server.ts"
	]
}
